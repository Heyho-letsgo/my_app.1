<%- model_class = Acqereurrecherche -%>
<div class="page-header">
  <h1><%=t '.title', :default => Acqereurrecherche %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th><%= model_class.human_attribute_name(:type_de_bien) %></th>
      <th><%= model_class.human_attribute_name(:chambre) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @acqereurrecherches.each do |acqereurrecherche| %>
      <tr>
        <td><%= acqereurrecherche.id %></td>
        <td><%= acqereurrecherche.type_de_bien %></td>
        <td><%= acqereurrecherche.chambre %></td>
        <td><%=l acqereurrecherche.created_at, format: :long %></td>
        <td>
          <%= link_to acqereurrecherche_path(acqereurrecherche), :class => 'btn btn-xs', :title => "#{ t('.show', :default => t('helpers.links.show')) }" do %>
            <%= glyph 'info-sign' %>
            <%= glyph 'pencil' %>
          <%- end -%>
          <%= link_to acqereurrecherche_path(acqereurrecherche), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-xs', :title => "#{ t('.destroy', :default => t('helpers.links.destroy')) }" do %>
            <%= glyph 'remove' %>
          <%- end -%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_acqereurrecherche_path,
            :class => 'btn btn-primary' %>

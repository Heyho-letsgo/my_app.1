<%- model_class = Acqereurrecherche -%>
<div class="page-header">
  <h1><%=t '.title', :default => Acqereurrecherche %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:type_de_bien) %></dt>
      <dd><pre class="prettyprint"><%= @acqereurrecherche.type_de_bien %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:chambre) %></dt>
      <dd><pre class="prettyprint"><%= @acqereurrecherche.chambre %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              acqereurrecherches_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_acqereurrecherche_path(@acqereurrecherche), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              acqereurrecherche_path(@acqereurrecherche),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

